<launch>
    <arg name='host' default='localhost'/>
    <arg name='port' default='2000'/>
    <include file="$(find carla_ros_bridge)/launch/client.launch">
        <arg name='host' default='$(arg host)'/>
        <arg name='port' default='$(arg port)'/>
    </include>
    <include file="$(find carla_ego_vehicle)/launch/carla_example_ego_vehicle.launch">
        <arg name='host' default='$(arg host)'/>
        <arg name='port' default='$(arg port)'/>
    </include>
    <include file="$(find carla_ros_manual_control)/launch/carla_ego_manual_control.launch"  pass_all_args="True"/>
</launch>
